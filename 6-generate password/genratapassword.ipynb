    "import random as rnd\n",
    "name = input(\"Please enter your full name (without any space): \")\n",
    "passw = \"\"\n",
    "for i in range(3):\n",
    "  randIndex = rnd.randint(0, len(name)-1)\n",
    "  letter = name[randIndex]\n",
    "  passw += letter.lower()\n",
    "randNum = rnd.randint(1000,9999)\n",
    "passw += str(randNum)\n",
    "print(passw)"
   ]
  }
